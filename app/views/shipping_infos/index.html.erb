<h1>Listing shipping_infos</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Contact number</th>
      <th>Email</th>
      <th>City</th>
      <th>Township</th>
      <th>Shipping address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipping_infos.each do |shipping_info| %>
      <tr>
        <td><%= shipping_info.full_name %></td>
        <td><%= shipping_info.contact_number %></td>
        <td><%= shipping_info.email %></td>
        <td><%= shipping_info.city %></td>
        <td><%= shipping_info.township %></td>
        <td><%= shipping_info.shipping_address %></td>
        <td><%= link_to 'Show', shipping_info %></td>
        <td><%= link_to 'Edit', edit_shipping_info_path(shipping_info) %></td>
        <td><%= link_to 'Destroy', shipping_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping info', new_shipping_info_path %>
